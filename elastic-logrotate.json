{
  "env": {
"INDEX_NAME": "default",
"VAULT_HOST": "vault.service.eos.dev.air.hospitales.sanitas.azure",
"MAX_AGE": "1d",
"MAX_DOCS": "10",
"MAX_SIZE": "5gb",
"ELASTIC_HOST": "https://coordinator.elasticsearchstratio.l4lb.thisdcos.directory",
"ELASTIC_PORT": "9200",
"DAYS_COUNT": "1"
  },
  "id": "/elastic-logrotate",
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "container": {
    "portMappings": [
      {
        "containerPort": 0,
        "hostPort": 0,
        "protocol": "tcp",
        "servicePort": 10105,
        "name": "default"
      }
    ],
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "master.mesos:5001/stratio/elastic-logrotate:test",
      "forcePullImage": false,
      "privileged": false,
      "parameters": []
    }
  },
  "cpus": 0.5,
  "disk": 0,
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 3,
      "portIndex": 0,
      "timeoutSeconds": 20,
      "delaySeconds": 15,
      "protocol": "TCP",
      "ipProtocol": "IPv4"
    }
  ],
  "instances": 1,
  "maxLaunchDelaySeconds": 3600,
  "mem": 128,
  "secrets": {
    "role": {
      "source": "open"
    }
  },
  "gpus": 0,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 0,
    "expungeAfterSeconds": 0
  },
  "fetch": [],
  "constraints": []
}